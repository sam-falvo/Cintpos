# This tests the code generated by gcc for various archtectures

run:
	gcc -O3 -S t5.c
	mv t5.s t5.default
	gcc -O3 -march=native -S t5.c
	mv t5.s t5.native
	gcc -O3 -march=i386 -S t5.c
	mv t5.s t5.i386
	gcc -O3 -march=i486 -S t5.c
	mv t5.s t5.i486
	gcc -O3 -march=i686 -S t5.c
	mv t5.s t5.i686
	gcc -O3 -march=pentiumpro -S t5.c
	mv t5.s t5.pentiumpro
	gcc -O3 -march=corei7 -S t5.c
	mv t5.s t5.corei7
	gcc -O3 -march=i686 -o t5 t5.c


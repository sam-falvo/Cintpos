.k arg

# Assume bcpl c echo and preload are already compiled

preload bcpl c echo

echo "First compile the bigender versions"
c bsb boot "<arg>"
c bsb blib "<arg>"
c bsb klib "<arg>"
c bsb dlib "<arg>"
c bsb idle "<arg>"
c bsb cli "<arg>"
c bsb cli_init "<arg>"
c bsb debug "<arg>"
c bsb cohand "<arg>"
c bsb fh0 "<arg>"
c bsb mbxhand "<arg>"
c bsb tcphand "<arg>"

c bcb tbcpl "<arg>"
c bcb bcpl "<arg>"
c bcb c "<arg>"
c bcb echo "<arg>"
c bcb logout "<arg>"
c bcb cmpltest "<arg>"
c bcb map "<arg>"
c bcb abort "<arg>"


echo "Now compile for the little ender versions"
c bsl boot "<arg>"
c bsl blib "<arg>"
c bsl klib "<arg>"
c bsl dlib "<arg>"
c bsl idle "<arg>"
c bsl cli "<arg>"
c bsl cli_init "<arg>"
c bsl debug "<arg>"
c bsl cohand "<arg>"
c bsl fh0 "<arg>"
c bsl mbxhand "<arg>"
c bsl tcphand "<arg>"

c bcl tbcpl "<arg>"
c bcl bcpl "<arg>"
c bcl c "<arg>"
c bcl echo "<arg>"
c bcl logout "<arg>"
c bcl cmpltest "<arg>"
c bcl map "<arg>"
c bcl abort "<arg>"

echo "Now compile the system files"
c bs boot "<arg>"
c bs blib "<arg>"
c bs klib "<arg>"
c bs dlib "<arg>"
c bs idle "<arg>"
c bs cli "<arg>"
c bs cli_init "<arg>"
c bs debug "<arg>"
c bs cohand "<arg>"
c bs fh0 "<arg>"
c bs mbxhand "<arg>"
c bs tcphand "<arg>"


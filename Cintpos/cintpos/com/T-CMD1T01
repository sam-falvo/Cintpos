#!/home/mr/distribution/BCPL/cintcode/cintsys -s
echo "bcpl com/abort.b to enderbig/abort hdrs BCPLHDRS oender t32 hard"
bcpl com/abort.b to enderbig/abort hdrs BCPLHDRS oender t32 hard
c bcb bcpl "hard"
c bsb blib "hard"
c bsb boot "hard"
c bcb c "hard"
